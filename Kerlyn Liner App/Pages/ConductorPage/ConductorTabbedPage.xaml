             x:Class="Kerlyn_Liner_App.Pages.ConductorPage.ConductorTabbedPage"
            xmlns:local="clr-namespace:Kerlyn_Liner_App.Pages.ConductorPage"
            BarBackgroundColor="#0066CC" BarTextColor="White"
            xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"
